<Application x:Class="GS.Apps.OrderSystem.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:commands="clr-namespace:GS.Libraries.Commands;assembly=GS.Libraries.Commands"
             xmlns:models="clr-namespace:GS.Libraries.Models;assembly=GS.Libraries.Models"
             Startup="HandleApplicationStartup">
    <Application.Resources>
        <models:Customers x:Key="customers" />
        <models:Orders x:Key="orders" />
        
        <commands:LoadCustomersCommand x:Key="loadCustomers" />
        <commands:LoadOrdersCommand x:Key="loadOrders" />
    </Application.Resources>
</Application>
